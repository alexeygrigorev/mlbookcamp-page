---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{site.url}}/</loc>
    </url>
    <url>
        <loc>{{site.url}}/articles</loc>
    </url>
    <url>
        <loc>{{site.url}}/courses</loc>
    </url>
    {% for post in site.article %}
    <url>
        <loc>{{site.url}}{{ post.url }}</loc>
    </url>
    {% endfor %}
    {% for post in site.course %}
    <url>
        <loc>{{site.url}}{{ post.url }}</loc>
    </url>
    {% endfor %}
</urlset>